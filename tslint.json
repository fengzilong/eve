{
	"extends": "tslint:recommended",
	"rules": {
		"indent": [ true, "tabs" ],
		"quotemark": [ true, "single" ],
		"interface-name": [ true, "always-prefix" ],
		"no-bitwise": false,
		"no-empty": false,
		"only-arrow-functions": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"variable-name": false,
		"no-use-before-declare": false,
		"semicolon": false,
		"trailing-comma": [
			true,
			{
				"multiline": {
					"objects": "always",
					"arrays": "always",
					"functions": "never",
					"typeLiterals": "ignore"
				},
				"singleline": "never"
			}
		]
	}
}
